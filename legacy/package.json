{
  "name": "re-resume",
  "version": "0.1.0",
  "description": "Another front end engineer resume generator",
  "private": true,
  "scripts": {
    "format": "prettier --no-semi --single-quote --trailing-comma es5 --jsx-bracket-same-line --write 'src/**/*.{js,jsx}'",
    "start": "npm run dev",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "lint": "eslint src webpack.config.js",
    "dev": "npm run env:dev -- webpack-dev-server",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run env:prod -- webpack",
    "build-resume": "PUBLIC_PATH=resume npm run build && cp -r dist/ ../blog/resume/"
  },
  "author": {
    "name": "cool4zbl",
    "email": "i@zhangbinliu.me"
  },
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "3.4.12",
    "@storybook/react": "3.4.12",
    "@webpack-blocks/dev-server2": "0.4.0",
    "@webpack-blocks/webpack2": "0.4.0",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.5",
    "babel-plugin-styled-components": "1.13.3",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "copyfiles": "1.2.0",
    "cross-env": "5.2.1",
    "css-loader": "0.28.11",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-no-unused-vars-rest": "1.0.6",
    "eslint-plugin-react": "7.31.8",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.30.1",
    "opn-cli": "3.1.0",
    "raw-loader": "0.5.1",
    "react-hot-loader": "3.1.3",
    "rimraf": "2.7.1",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "2.7.0",
    "webpack-blocks-happypack": "0.1.3",
    "webpack-blocks-split-vendor": "0.2.1",
    "webpack-dev-server": "2.11.5"
  },
  "dependencies": {
    "history": "4.10.1",
    "lodash": "4.17.21",
    "normalizr": "3.6.2",
    "prettier": "1.19.1",
    "prop-types": "15.8.1",
    "react": "15.7.0",
    "react-dom": "15.7.0",
    "react-helmet": "5.2.1",
    "react-router-dom": "4.3.1",
    "react-typography": "0.15.10",
    "styled-components": "2.4.1",
    "styled-theme": "0.3.3",
    "styled-tools": "0.1.5",
    "typography": "0.15.12",
    "typography-plugin-code": "0.15.11",
    "typography-theme-funston": "0.15.10",
    "typography-theme-github": "0.15.10",
    "typography-theme-stow-lake": "0.15.10"
  },
  "keywords": [
    "react",
    "resume",
    "online resume",
    "F2E resume",
    "front end engineer resume"
  ]
}
